{
  "name": "whatsapp-sheet-bot",
  "version": "2.0.0",
  "description": "Self-hosted WhatsApp notification bot with real-time webhook integration for Google Sheets",
  "main": "src/core/bot.js",
  "scripts": {
    "start": "node src/core/bot.js",
    "dev": "node src/core/bot.js",
    "test": "node scripts/test-bot.js",
    "test:developer": "node scripts/test-developer-whitelist.js",
    "pm2:start": "pm2 start config/ecosystem.config.js",
    "pm2:stop": "pm2 stop whatsapp-sheet-bot",
    "pm2:restart": "pm2 restart whatsapp-sheet-bot",
    "pm2:logs": "pm2 logs whatsapp-sheet-bot",
    "pm2:status": "pm2 status",
